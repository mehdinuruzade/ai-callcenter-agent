{
  "name": "ai-callcenter-agent",
  "version": "1.0.0",
  "description": "Multi-domain AI Call Center Agent with Admin Panel",
  "scripts": {
    "dev": "npm run build:server && node server.js",
    "dev:watch": "concurrently \"npm run build:server -- --watch\" \"nodemon server.js\"",
    "build": "npm run build:server && next build",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production npm run build:server && node server.js",
    "twilio:phone":"twilio dev-phone",
    "lint": "next lint",
    "dev:ngrok": "ngrok http 3000",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^2.2.0",
    "@prisma/client": "^6.19.0",
    "ai": "^5.0.93",
    "axios": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "date-fns": "^3.0.0",
    "dotenv": "^16.4.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.2.0",
    "next-auth": "^4.24.13",
    "openai": "^4.52.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hot-toast": "^2.6.0",
    "twilio": "^5.0.0",
    "ws": "^8.17.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.19.25",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.5.10",
    "autoprefixer": "^10.4.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.0",
    "prisma": "^6.19.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "ngrok": "^4.3.3",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
